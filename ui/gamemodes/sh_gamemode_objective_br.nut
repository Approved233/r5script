const int FULL_PERCENT = 100
array< featureTutorialTab > function ObjectiveBR_PopulateAboutText()
{
	array< featureTutorialTab > tabs
	string playlistUiRules = GetCurrentPlaylist_UIRules()

	if ( playlistUiRules != GAMEMODE_OBJECTIVE_BR )
		return tabs

	featureTutorialTab tab1
	featureTutorialTab tab2

	array< featureTutorialData > tab1Rules
	array< featureTutorialData > tab2Rules

	string currentPlaylist = GamemodeUtility_GetPlaylist()

	
	tab1.tabName = "#GAMEMODE_RULES_OVERVIEW_TAB_NAME"
	tab1Rules.append( UI_FeatureTutorialDialog_BuildDetailsData( "#GAMEMODE_OBJECTIVE_BR_ABOUT_HEADER_1", "#GAMEMODE_OBJECTIVE_BR_ABOUT_BODY_1", $"rui/hud/gametype_icons/ltm/objectivebr/about_objectivebr_1" ) )
	tab1Rules.append( UI_FeatureTutorialDialog_BuildDetailsData( "#GAMEMODE_OBJECTIVE_BR_ABOUT_HEADER_2", "#GAMEMODE_OBJECTIVE_BR_ABOUT_BODY_2", $"rui/hud/gametype_icons/ltm/objectivebr/about_objectivebr_2" ) )
	string t1Body3Text = Localize( "#GAMEMODE_OBJECTIVE_BR_ABOUT_BODY_3", string( OBJECTIVE_BR_FINAL_OBJECTIVE_ROUND ) )
	tab1Rules.append( UI_FeatureTutorialDialog_BuildDetailsData( "#GAMEMODE_OBJECTIVE_BR_ABOUT_HEADER_3", t1Body3Text, $"rui/hud/gametype_icons/ltm/objectivebr/about_objectivebr_3" ) )
	tab1.rules = tab1Rules

	
	tab2.tabName = "#GAMEMODE_OBJECTIVE_BR_TAB2_HEADER"

		table< string, string> scoringTableData = {}
		string t2Body1Item1Text = Localize( "#GAMEMODE_OBJECTIVE_BR_ABOUT_T2_BODY_1_ITEM_1", string( FULL_PERCENT / maxint( GetPlaylistVarInt( currentPlaylist, "objective_br_capture_fortify_levels", 0 ), 1 ) ) )
		scoringTableData[t2Body1Item1Text] <- Localize( "#GAMEMODE_OBJECTIVE_BR_RULES_POINTS", string( GetPlaylistVarInt( currentPlaylist, "objective_br_score_capture_progress", DEFAULT_SCORE_CAPTURE_PROGRESS ) ) )
		scoringTableData["#GAMEMODE_OBJECTIVE_BR_ABOUT_T2_BODY_1_ITEM_2"] <- Localize( "#GAMEMODE_OBJECTIVE_BR_RULES_POINTS", string( GetPlaylistVarInt( currentPlaylist, "objective_br_score_capture_complete", DEFAULT_SCORE_CAPTURE_COMPLETE ) ) )
		tab2Rules.append( UI_FeatureTutorialDialog_BuildDetailsData( "#GAMEMODE_OBJECTIVE_BR_ABOUT_T2_HEADER_1", "#GAMEMODE_OBJECTIVE_BR_ABOUT_T2_BODY_1", $"rui/hud/gametype_icons/ltm/objectivebr/about_objectivebr_4", [], scoringTableData ) )


	table< string, string> evoScoringTableData = {}
	string t2Body2Text = Localize( "#GAMEMODE_OBJECTIVE_BR_ABOUT_T2_BODY_2", string( GetPlaylistVarInt( currentPlaylist, "objective_br_evo_score_requirement", DEFAULT_EVO_THRESHOLD_FOR_SCORE ) ), string( GetPlaylistVarInt( currentPlaylist, "objective_br_evo_score_catchup_0", DEFAULT_SCORE_EVO ) ) )
	string t2Body2Item1Text = Localize( "#GAMEMODE_OBJECTIVE_BR_ABOUT_T2_BODY_2_ITEM_1", string( GetPlaylistVarInt( currentPlaylist, "objective_br_evo_score_requirement", DEFAULT_EVO_THRESHOLD_FOR_SCORE ) ) )
	evoScoringTableData[t2Body2Item1Text] <- Localize( "#GAMEMODE_OBJECTIVE_BR_RULES_POINTS", string( GetPlaylistVarInt( currentPlaylist, "objective_br_evo_score_catchup_0", DEFAULT_SCORE_EVO ) ) )
	tab2Rules.append( UI_FeatureTutorialDialog_BuildDetailsData( "#GAMEMODE_OBJECTIVE_BR_ABOUT_T2_HEADER_2", t2Body2Text, $"rui/hud/gametype_icons/ltm/objectivebr/about_objectivebr_5", [], evoScoringTableData ) )

	tab2Rules.append( UI_FeatureTutorialDialog_BuildDetailsData( "#GAMEMODE_OBJECTIVE_BR_ABOUT_T2_HEADER_3", "#GAMEMODE_OBJECTIVE_BR_ABOUT_T2_BODY_3", $"rui/hud/gametype_icons/ltm/objectivebr/about_objectivebr_6" ) )
	tab2.rules = tab2Rules


	tabs.append( tab1 )
	tabs.append( tab2 )

	return tabs
}































































































































































bool function ObjectiveBR_IsRedeployingForSuddenDeath()
{
	if ( GameModeVariant_IsActive( eGameModeVariants.SURVIVAL_OBJECTIVE_BR ) && GetGameState() == eGameState.Playing && ( GetGlobalNetInt( "objective_br_gameplay_phase" ) == eObjectiveBRGameplayPhase.REDEPLOY || GetGlobalNetInt( "objective_br_gameplay_phase" ) == eObjectiveBRGameplayPhase.SUDDEN_DEATH ) )
		return true

	return false
}









































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































      